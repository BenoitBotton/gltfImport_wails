      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/CHASEdesktop.exe",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}",
      "hideSystemGoroutines":true,
    },
    {
      "name": "Wails: Dev CHASEdesktop",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/CHASEdesktop.exe",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}",
      "hideSystemGoroutines":true,
    },
  ]
}